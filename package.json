{
  "name": "issue_tracking_system",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "json-server --routes ./mock/routes.json --watch db.json",
    "start:dev": "ng serve",
    "start:one": "ng serve -c=one",
    "start:two": "ng serve -c=two",
    "build": "ng build",
    "build:prod": "ng build --configuration production",
    "test": "ng test",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:ts": "ng lint",
    "lint:fix": "ng lint --fix",
    "lint:scss": "stylelint \"./src/**/*.scss\"",
    "e2e": "ng e2e",
    "test-headless": "ng test --watch=false --browsers=ChromeHeadless",
    "mock": "json-server --routes ./mock/routes.json --watch db.json",
    "concurrently": "concurrently --kill-others \"npm run mock\" \"npm run start:dev\""
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.1",
    "@angular/cdk": "^15.0.0",
    "@angular/common": "^15.0.1",
    "@angular/compiler": "^15.0.1",
    "@angular/core": "^15.0.1",
    "@angular/forms": "^15.0.1",
    "@angular/material": "^15.0.0",
    "@angular/platform-browser": "^15.0.1",
    "@angular/platform-browser-dynamic": "^15.0.1",
    "@angular/router": "^15.0.1",
    "@ngrx/effects": "^15.0.0",
    "@ngrx/entity": "^15.0.0",
    "@ngrx/store": "^15.0.0",
    "@ngrx/store-devtools": "^15.0.0",
    "material-icons-font": "^2.0.0",
    "ngx-progressbar": "^6.0.2",
    "rxjs": "^6.5.3",
    "tslib": "^2.0.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.2",
    "@angular/cli": "^15.0.2",
    "@angular/compiler-cli": "^15.0.1",
    "@angular/language-service": "^15.0.1",
    "@types/jasmine": "~3.6.0",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "^12.11.1",
    "codelyzer": "^6.0.0",
    "concurrently": "^7.6.0",
    "faker": "^4.1.0",
    "jasmine-core": "~3.6.0",
    "jasmine-spec-reporter": "~5.0.0",
    "json-server": "^0.16.1",
    "karma": "~6.4.1",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage-istanbul-reporter": "~3.0.2",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "^1.5.0",
    "protractor": "~7.0.0",
    "stylelint": "^13.3.3",
    "stylelint-config-sass-guidelines": "^7.0.0",
    "ts-node": "~8.3.0",
    "tslint": "~6.1.0",
    "typescript": "~4.8.4"
  },
  "engines": {
    "node": "16.15.0",
    "npm": "8.5.5"
  },
  "resolutions": {
    "webpack": "^5.0.0"
  }
